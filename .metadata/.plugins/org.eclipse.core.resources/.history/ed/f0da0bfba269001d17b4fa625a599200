package practice;

public class Find {
	public static void main(String[] args) {
		
		int m=1000000000;
		int n=10;
		int total=0;
		boolean status=true;
int i=1;
while(total<m)
{
	int temp=i*n;
	total+=temp;
	i++;
	if(total==m)		
	{
		System.out.println("true");
		status=true;
	}	

	if(status==false)
	{
		System.out.println("false");
	}
}


	}

}
